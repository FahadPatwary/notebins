{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
      { 
        "source": "/api/:path*", 
        "destination": "https://notebinsbackendserver.azurewebsites.net/api/:path*" 
      },
      { 
        "source": "/(.*)", 
        "destination": "/index.html" 
      }
    ]
  }